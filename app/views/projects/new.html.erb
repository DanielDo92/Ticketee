<h1> New Project</h1>
<%= form_for(@project) do |f| %>
  <p>
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </p>

  <p>
    <%= f.label :description %><br>
    <%= f.text_field :description %>
  </p>

  <%= f.submit %> <br><br>
  #For learning purpose only<br>
  <%= f.submit :Clear, type: :reset %>
  <%= f.submit :Cancel, name: :cancel%>
<% end %>

